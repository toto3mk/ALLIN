/*Create array (10, 20, 4, 45, 99) then , Find Second Largest Element  
Without Sorting: */

public class tt {
    public static void main(String[] args) {

        int n[] = { 10, 20, 4, 45, 99 };
        int l = n[0];
        int sl = n[0];

        for (int i = 1; i < n.length; i++) {
            if (n[i] > l) {
                sl = l; // 10 //
                l = n[i]; // 20 // 20
            } else if (n[i] > sl) {
                sl = n[i];
            }
            else if ( l == sl){
                sl = n[i]; // 20
            }
        }

        System.out.println("sec largest element is: " + sl);

    }
}


