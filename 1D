/*create integer array ( 1 ,2 ,2 ,3 ,3 ,3) and count the frequancy of each element */
// another way of solving it ******************

public class tt {
    public static void main(String[] args) {

        int n[] = { 1, 2, 2, 3, 3, 3 }; //6
        int f[] = new int[6]; // 6 = n.length
        int c = 0; // 1

        for (int i = 0; i < n.length; i++) {
            int t = 0;
            for (int j = 0; j < n.length; j++) { 
                if (n[i] == n[j]) {
                    t++;
                }
            }// first loop : t = 1 // second loop : t = 2 

            boolean d = false;
            for (int j = 0; j < c; j++) {
                if (n[i] == f[j]) {
                    d = true;
                    break;
                }
            }

            if (!d) { // if d is false
                f[c] = n[i]; // store n[i] in f[c]
                c++;
                System.out.println(n[i] + " appears " + t + " times");
            }
        }

    }
}

