/* Create integer array (0, 1, 0, 3, 12) then Shift All Zeroes to the End:*/
public class tt {
    public static void main(String[] args) {

        int[] n = { 0, 1, 0, 3, 12 };
        int c = 0;

        for (int i = 0; i < n.length; i++) {
            if (n[i] != 0) {
                n[c] = n[i];
                c++;
            }
        }

        while (c < n.length) {
            n[c] = 0;
            c++;
        }

        for (int i = 0; i < n.length; i++) {
            System.out.println(n[i]);
        }

    }
}

